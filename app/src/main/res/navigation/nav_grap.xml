<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/nav_graph"
            app:startDestination="@id/loginFragment">

    <fragment android:id="@+id/loginFragment"
              android:name="org.sic4change.chuqabp.ui.LoginFragment"
              android:label="@string/app_name"
              tools:layout="@layout/fragment_login">
        <action android:id="@+id/action_medicamentosFragment_to_medicamentoDetailFragment"
                app:destination="@id/createAccount" app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim" app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment android:id="@+id/createAccount"
              android:name="org.sic4change.chuqabp.ui.CreateAccountFragment"
              android:label=""
              tools:layout="@layout/fragment_create_account">
        <argument android:name="topicId" app:argType="string"/>
    </fragment>
</navigation>